{
  "name": "itunes-cart-bb",
  "private": true,
  "version": "0.0.1",
  "description": "Itunes cart with backbone",
  "scripts": {
    "start": "webpack-dev-server --env development",
    "test": "jest",
    "test-dev": "npm test -- --watch"
  },
  "repository": "",
  "keywords": [],
  "author": "Gachet Anthony",
  "dependencies": {
    "backbone": "^1.3.3",
    "handlebars": "^4.0.10",
    "jquery": "^3.2.1",
    "material-design-lite": "^1.3.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "@types/backbone": "^1.3.34",
    "@types/handlebars": "^4.0.33",
    "@types/jest": "^20.0.2",
    "@types/jquery": "^3.2.5",
    "@types/node": "^8.0.7",
    "@types/underscore": "^1.8.1",
    "@types/webpack": "^3.0.5",
    "autoprefixer": "^7.1.2",
    "awesome-typescript-loader": "^3.2.1",
    "backbone-decorators": "0.0.6",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "handlebars-loader": "^1.5.0",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-plugin": "^2.29.0",
    "jest": "^20.0.4",
    "postcss-loader": "^2.0.6",
    "precss": "^2.0.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "ts-jest": "^20.0.6",
    "ts-node": "^3.3.0",
    "tslint": "latest",
    "typescript": "^2.3.4",
    "typings-for-css-modules-loader": "^1.5.0",
    "webpack": "^3.3.0",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-dev-server": "^2.6.1",
    "webpack-dotenv-plugin": "^2.0.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "testPathIgnorePatterns": [
      ".hbs"
    ],
    "testURL": "http://jest.testing.url/",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(hbs)$": "<rootDir>/__mocks__/templateMock.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
